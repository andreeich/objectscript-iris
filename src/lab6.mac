  #;     Write !
  #; }

  #; get the librarian count
  Set librarianCount = ##class(Library.Librarian).GetLibrarianCount()
  Write !, "Librarian count: ", librarianCount

  #; get librarians by Position
  Set librarians = ##class(Library.Librarian).GetLibrarianByPosition("Head Librarian")
  While librarians.Next() {
      Write !, "Name: ", librarians.Get("Name")
      Write !, "Position: ", librarians.Get("Position")
      Write !
  }
  Do librarians.Close()

